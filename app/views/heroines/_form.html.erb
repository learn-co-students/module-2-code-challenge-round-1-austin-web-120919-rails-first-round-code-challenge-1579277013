<%= form_for @heroine do |f| %>
    <%= f.label 'Name: '%>
    <%= f.text_field :name %>
    <%= f.label 'Super Name: '%>
    <%= f.text_field :super_name %>
    <%= f.label 'Powers: '%>
    <%# #<%= f.collection_select(:powers, @powers, :id, :name) %>
    <%= f.fields_for :powers do |ff| %>
        <%= ff.collection_check_boxes(:powers, @powers, :id, :name %>
    <% end %>

    <%= f.submit %>
<% end %>
                

